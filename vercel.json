{
    "version": 2,
    "builds": [
      {
        "src": "dist/antidote/server/main.js",
        "use": "@vercel/node"
      },
      {
        "src": "dist/antidote/browser/**",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "dist/antidote/server/main.js"
      }
    ]
}